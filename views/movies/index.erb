<h1>Movies</h1>

<table style="width:100%">
  <tr>
    <th>Title</th>
    <th>Year Released</th>
    <th>Rating</th>
    <th>Genre</th>
    <th>Studio</th>
  </tr>
  <% @movies.each do |movie| %>
    <tr>
      <td><a href="/movies/<%= movie['id'] %>"><%= movie['title'] %></a></td>
      <td><%= movie['year'] %></td>
      <td><%= movie['rating'] %></td>
      <td><%= movie['genre'] %></td>
      <td><%= movie['studio'] %></td>
    </tr>
  <% end %>
</table>
